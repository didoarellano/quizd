{
  "name": "quiz",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "preview": "vite preview",
    "emulators": "firebase -c firebase/firebase.json emulators:start --import firebase/emulator-data --export-on-exit firebase/emulator-data",
    "functions_watcher": "cd firebase/functions && npm run build:watch"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.1",
    "@tanstack/react-query": "^5.59.9",
    "@tanstack/react-query-devtools": "^5.59.9",
    "@types/mdast": "^4.0.4",
    "@uiw/react-textarea-code-editor": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^10.13.1",
    "lucide-react": "^0.473.0",
    "mdast-util-from-markdown": "^2.0.1",
    "mdast-util-frontmatter": "^2.0.1",
    "mdast-util-gfm": "^3.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "micromark-extension-frontmatter": "^2.0.0",
    "micromark-extension-gfm": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "wouter": "^3.3.5",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@firebase/rules-unit-testing": "^4.0.1",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "firebase-tools": "^13.27.0",
    "globals": "^15.9.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.6"
  },
  "packageManager": "pnpm@9.1.0",
  "pnpm": {
    "overrides": {
      "rollup": "npm:@rollup/wasm-node"
    },
    "supportedArchitectures": {
      "os": [
        "linux",
        "darwin"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  }
}
